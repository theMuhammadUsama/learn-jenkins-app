{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnJenkinsFile",
            "image": "767398117694.dkr.ecr.ap-southeast-1.amazonaws.com/learnjenkinsapp:1.0.16",
            "portMappings": [{
                "name":"nginx-80-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::767398117694:role/ecsTaskExecutionRole"
}